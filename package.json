{
  "name": "profile-maker",
  "version": "1.0.0",
  "description": "A profile maker where you can create and make edits to your profile picture, name, and description.",
  "main": "server.js",
  "engines": {
    "node": "6.0.0"
  },
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive ./test",
    "test:watch": "npm run test -- --watch",
    "postinstall": "rimraf dist && cross-env NODE_ENV=production webpack -p --config ./webpack.production.config.js --progress --profile --colors",
    "start": "node server.js"
  },
